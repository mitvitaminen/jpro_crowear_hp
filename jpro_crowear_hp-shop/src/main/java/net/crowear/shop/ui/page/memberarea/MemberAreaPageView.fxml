<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE VBox>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox xmlns:fx="http://javafx.com/fxml/1"
	fx:controller="net.chrisrocholl.homepage.ui.page.memberarea.MemberAreaPageView"
	styleClass="lightGreyBackground">	
	<HBox>
		<GridPane VBox.vgrow="ALWAYS" HBox.hgrow="ALWAYS">
			<padding>
				<Insets top="10" bottom="10" left="5" right="5" />
			</padding>
			<columnConstraints>
				<ColumnConstraints percentWidth="100.0" />
			</columnConstraints>
			<rowConstraints>
				<RowConstraints percentHeight="100.0" />
			</rowConstraints>
			<VBox GridPane.rowIndex="0" GridPane.columnIndex="0">
				<fx:include
					source="../../component/content/member/MemberContentView.fxml" />
			</VBox>
		</GridPane>
	</HBox>
	<fx:include
		source="../../component/statusbar/StatusBarView.fxml" />
</VBox>